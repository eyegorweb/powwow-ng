{
  "name": "powwow-ng-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode fullstack",
    "serve:mock": "yarn run server:mock | vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "yarn lint && yarn test:unit --coverage",
    "lint": "vue-cli-service lint",
    "build:full-local": "vue-cli-service build --mode full-local",
    "build:integration": "vue-cli-service build --mode integration",
    "serve:local-integration": "vue-cli-service serve --mode local-integration",
    "serve:fullstack": "vue-cli-service serve --mode fullstack",
    "server:mock": "node ./server-mock/index.js",
    "serve:prod": "yarn build --mode fullstack && serve -s dist -l 8081",
    "test:dev": "yarn test:unit --watchAll",
    "test:unit": "vue-cli-service test:unit",
    "e2e:open": "cypress open ",
    "e2e:open:focus": "cypress open -e TAGS=\"@focus\"",
    "e2e": "yarn run clean:reports && ./node_modules/.bin/cypress-tags run -e TAGS=\"not @ignore\"",
    "e2e:focus": "./node_modules/.bin/cypress-tags run -e TAGS=\"@focus\"",
    "e2e:dashboard": "./node_modules/.bin/cypress-tags run -e TAGS=\"not @ignore\" --record --key ci-tests --parallel ",
    "clean:reports": "rm -rf mochawesome-report/ TestReport cypress-tests-report.json",
    "merge:reports": "mochawesome-merge mochawesome-report/*.json > cypress-tests-report.json",
    "create:html:report": "marge --reportDir TestReport cypress-tests-report.json",
    "generate:html:report": "yarn run merge:reports && yarn run create:html:report"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "bootstrap": "^4.1.3",
    "cypress": "^5.3.0",
    "cypress-cucumber-preprocessor": "^2.0.1",
    "cypress-waitfor": "^1.1.0",
    "daterangepicker": "^3.0.3",
    "flatpickr": "^4.5.7",
    "fuzzysort": "^1.1.4",
    "graphql": "^14.0.2",
    "highcharts": "^7.1.3",
    "highcharts-vue": "^1.3.5",
    "jest-transform-stub": "^2.0.0",
    "jest-when": "^3.2.1",
    "jquery": "^3.3.1",
    "jquery-ui-dist": "^1.12.1",
    "lodash": "^4.17.11",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.differenceby": "^4.8.0",
    "lodash.differencewith": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.maxby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.startswith": "^4.2.1",
    "lodash.union": "^4.6.0",
    "lodash.unionby": "^4.8.0",
    "lodash.xorwith": "^4.5.0",
    "moment": "^2.24.0",
    "sass": "^1.32.11",
    "serve": "^11.2.0",
    "snapsvg-cjs": "^0.0.6",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-fragment": "1.5.1",
    "vue-grid-layout": "^2.3.7",
    "vue-i18n": "^8.0.0",
    "vue-promised": "^1.0.0",
    "vue-prop-data-fallback": "^1.0.1",
    "vue-router": "^3.0.2",
    "vue-slick": "^1.1.15",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1",
    "vuex-mock-store": "^0.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.26",
    "babel-core": "7.0.0-bridge.0",
    "compression-webpack-plugin": "^2.0.0",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-vue": "^7.9.0",
    "express": "^4.16.4",
    "faked-promise": "^2.0.0",
    "leaf-translations": "RPLeaf/rage-translations#master",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^8.4.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "postinstall-postinstall": "^2.0.0",
    "sass-loader": "^7.0.1",
    "shortid": "^2.2.8",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
